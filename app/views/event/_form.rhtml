<p><label for="event_state">State</label><br/>
<%= text_field 'event', 'state'  %></p>

<p><label for="event_capacity">Capacity</label><br/>
<%= text_field 'event', 'capacity'  %></p>

<p><label for="event_created_at">Created at</label><br/>
<%= date_select 'event', 'created_at'  %></p>

<p><label for="event_updated_at">Updated at</label><br/>
<%= date_select 'event', 'updated_at'  %></p>
