<%= error_messages_for 'event' %>

<!--[form:event]-->
<p><label for="event_name">Name</label><br/>
<%= text_field 'event', 'name'  %></p>

<p><label for="event_location">Location</label><br/>
<%= text_field 'event', 'location'  %></p>

<p><label for="event_date">Date</label><br/>
<%= date_select 'event', 'date'  %></p>

<p><label for="event_deadline">Deadline</label><br/>
